{
    "$schema": "https://json.schemastore.org/prettierrc",
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5",
    "tabWidth": 4,
    "printWidth": 150,
    "htmlWhitespaceSensitivity": "ignore",
    "proseWrap": "preserve",
    "overrides": [
        {
            "files": ["*.vue"],
            "options": {
                "parser": "vue"
            }
        }
    ]
}
